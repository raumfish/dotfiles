
Host n1mwph1.corp.cat.com
  IdentityFile              ~/.ssh/hpc_id_dsa
  IdentitiesOnly            yes
  User                      skrivsm
  Port                      22
#  Hostname                  n1mwph1.corp.cat.com
  PreferredAuthentications  publickey
  Compression               yes
  PubkeyAuthentication      yes
  PasswordAuthentication    no

Host github.com
  IdentityFile              ~/.ssh/git_id_rsa
  IdentitiesOnly            yes
  User                      git
  Port                      22
  Hostname                  github.com
  PreferredAuthentications  publickey
  Compression               no
  PubkeyAuthentication      yes
  PasswordAuthentication    no

Host *
  ForwardAgent              yes
  ForwardX11                yes
  Compression               yes
  CompressionLevel          6
  CheckHostIP               no
  TCPKeepAlive              yes
  LogLevel                  quiet
  StrictHostKeyChecking     no
  NumberOfPasswordPrompts   5
  VerifyHostKeyDNS          no
  Protocol                  2,1
  PreferredAuthentications  publickey,password
  HostbasedAuthentication   no
  PubkeyAuthentication      yes
  PasswordAuthentication    yes
  ControlPath               ~/.ssh/master-%r@%h:%p
  ControlMaster             auto

