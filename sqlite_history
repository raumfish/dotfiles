create table if not exists hpcusers.loginid(
lid_pk integer primary key,
lid_cws varchar2(10),
lid_hpc varchar2(10),
lid_name varchar2(50),
lid_mail varchar2(60),
lid_tcreate varchar2(23),
lid_talter varchar2(23)
);
